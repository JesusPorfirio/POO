[32m+     private void btnAdicionarItemActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnAdicionarItemActionPerformed[m
[32m+         Double qtdVendida = Double.valueOf(txtQtdeVendProd.getText());[m
[32m+         int x;[m
[32m+         if(qtdVendida > 0){[m
[32m+             for(x=0; x<cadProd.size(); x++){[m
[32m+                 if(cadProd.get(x).getCodigo().equals(txtCodProd.getText())){[m
