[32m+                             break;[m
[32m+                         }[m
[32m+              [m
[32m+                     }[m
[32m+                 }[m
[32m+                 Double valorItem = (novoItem.getProduto().getPreco()) * qtdVendida;[m
[32m+                 if(valorItem <= ((Cliente)cadCliVend.get(x)).getLimiteCred()){[m
[32m+                     String listaItem [] = { (novoItem.getProduto().getCodigo()),[m
[32m+                                              (novoItem.getProduto().getDescricao()),[m
[32m+                                              String.valueOf((novoItem.getProduto().getPreco())),[m
[32m+                                              txtQtdeVendProd.getText(),[m
[32m+                                              String.valueOf(valorItem)};[m
[32m+                     modTblProd.addRow(listaItem);[m
[32m+                    [m
[32m+                 }[m
[32m+             }else{[m
[32m+                 JOptionPane.showMessageDialog(null, "Quantidade solicitada maior que o estoque dispon√≠vel", "aviso", JOptionPane.ERROR_MESSAGE);[m
[32m+             }[m
