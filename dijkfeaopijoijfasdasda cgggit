[32m+     private void btnConsultaProdActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnConsultaProdActionPerformed[m
[32m+         String codProduto = txtCodProd.getText();[m
[32m+         int x;[m
[32m+         for(x=0; x<cadProd.size(); x++){[m
[32m+             if(cadProd.get(x).getCodigo().equals(codProduto)){[m
[32m+                 break;[m
[32m+             }[m
[32m+         }[m
[32m+         [m
[32m+         if(x < cadProd.size()){[m
[32m+             posProd = x;[m
[32m+         }else{[m
[32m+             posProd = -1;[m
[32m+         }[m
[32m+         [m
[32m+         if(posProd >= 0){[m
[32m+             txtConsultaProd.setText(cadProd.get(x).getDescricao());[m
[32m+             btnAdicionarItem.setEnabled(true);[m
[32m+             btnRemoverItem.setEnabled(true);[m
[32m+             txtQtdeVendProd.setEnabled(true);[m
[32m+             txtCodProd.setEnabled(false);[m
[32m+         }else{[m
[32m+             JOptionPane.showMessageDialog(null, "Produto n√£o cadastrado!", "aviso", JOptionPane.ERROR_MESSAGE);[m
[32m+         }[m
[32m+     }//GEN-LAST:event_btnConsultaProdActionPerformed[m
